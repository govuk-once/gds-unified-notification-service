{
  "name": "gds-unified-notification-service",
  "version": "0.0.0",
  "author": "",
  "type": "module",
  "scripts": {
    "build": "node esbuild.js",
    "commit": "cz",
    "development:sandbox:setup": "bun scripts/developer-sandbox-setup.ts",
    "format": "npm run prettier && npm run lint",
    "build": "bun scripts/build.ts",
    "lint": "npx eslint .  --fix",
    "prettier": "npx prettier . --write",
    "development:sandbox:setup": "bun scripts/developer-sandbox-setup.ts",
    "development:sandbox:release": "npm run build && cd terraform/notifications && terraform apply",
    "prettier:check": "npx prettier . --check",
    "semantic:release": "semantic-release",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@aws-sdk/client-kms": "3.948.0",
    "@aws-sdk/client-s3": "3.948.0",
    "@aws-sdk/client-sts": "3.948.0",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-angular": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@inquirer/prompts": "^8.0.2",
    "@types/archiver": "^7.0.0",
    "@types/aws-lambda": "^8.10.159",
    "@types/bun": "^1.3.4",
    "@types/http-errors": "^2.0.5",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/eslint-plugin": "^1.5.2",
    "archiver": "^7.0.1",
    "bun": "^1.3.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "prettier": "3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@aws-lambda-powertools/jmespath": "^2.29.0",
    "@aws-lambda-powertools/logger": "^2.29.0",
    "@aws-lambda-powertools/metrics": "^2.29.0",
    "@aws-lambda-powertools/parameters": "^2.29.0",
    "@aws-lambda-powertools/parser": "^2.29.0",
    "@aws-lambda-powertools/tracer": "^2.29.0",
    "@aws-lambda-powertools/validation": "^2.29.0",
    "@middy/core": "^6.4.5",
    "@middy/http-error-handler": "^6.4.5",
    "@middy/http-event-normalizer": "^6.4.5",
    "@middy/http-header-normalizer": "^6.4.5",
    "@middy/http-json-body-parser": "^6.4.5",
    "@middy/http-urlencode-body-parser": "^6.4.5",
    "@middy/secrets-manager": "^6.4.5",
    "http-errors": "^2.0.1",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.1.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": true
    }
  }
}
